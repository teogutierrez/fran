# Importar las bibliotecas necesarias
from selenium import webdriver
from selenium.webdriver.common.keys import Keys

# Abrir el navegador con Selenium
driver = webdriver.Chrome()
driver.get("https://prenotami.esteri.it/")

# Encontrar los elementos de entrada de usuario y contraseña
username = driver.find_element_by_name("email")
password = driver.find_element_by_name("password")

# Ingresar las credenciales y enviar el formulario
username.send_keys("fran.gaya6@gmail.com")
password.send_keys("Maradona10")
password.send_keys(Keys.RETURN)

# Encontrar el botón deseado y hacer clic en él
boton_deseado = driver.find_element_by_css_selector("AVANTI")
boton_deseado.click()

# Encontrar el botón deseado y hacer clic en él
boton_deseado = driver.find_element_by_css_selector("Prenota")
boton_deseado.click()

# Encontrar el botón deseado y hacer clic en él
boton_prenota = driver.find_element_by_css_selector("button.button-primary")
boton_prenota.click()



# Continuar con el resto del código para leer la hoja electrónica, etc.

# Cerrar el navegador
driver.quit()
